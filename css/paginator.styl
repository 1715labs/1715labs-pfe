.paginator
  text-align: center

  .paginator-page-selector
    display: inline-block
    margin: 0 10px

  .paginator-prev
    background: none
    color: FOREGROUND
    &:hover
      background: FAILURE
      color: #fff
      border-color: darken(FAILURE, 10%)

  .paginator-next
    background: none
    color: FOREGROUND
    &:hover
      background: SUCCESS
      color: #fff
      border-color: darken(SUCCESS, 10%)